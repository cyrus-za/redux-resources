{
	"name": "@approveme/redux-resources",
	"version": "1.0.8",
	"files": [
		"lib/**/*"
	],
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"keywords": [
		"redux",
		"approveme"
	],
	"author": "@approveme",
	"scripts": {
		"build": "tsc",
		"lint": "eslint '*/**/*.{js,ts}' --quiet --fix",
		"postversion": "git push && git push --tags",
		"prepare": "yarn build",
		"prepublishOnly": "yarn run prettier && yarn run lint",
		"prettier": "prettier  --write './**/*.{ts,js,jsx,css}'",
		"preversion": "yarn run prettier && yarn run lint",
		"version": "yarn run prettier && yarn run lint && git add -A src"
	},
	"private": false,
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"{src,test}/**/*.ts": [
			"pretty-quick --staged",
			"eslint --fix",
			"git add"
		],
		"{src,test}/**/*.{graphql,json,html,scss}": [
			"pretty-quick --staged",
			"git add"
		]
	},
	"dependencies": {
		"immutable": "^4.0.0-rc.12",
		"js-file-download": "^0.4.5",
		"redux-saga": "^1.1.1",
		"reselect": "^4.0.0"
	},
	"devDependencies": {
		"@types/axios": "^0.14.0",
		"@types/jest": "^24.0.18",
		"@types/lodash": "^4.14.144",
		"@types/node": "^12.7.12",
		"@types/react": "^16.9.9",
		"@typescript-eslint/eslint-plugin": "^2.4.0",
		"@typescript-eslint/parser": "^2.4.0",
		"eslint": "^6.5.1",
		"eslint-config-prettier": "^6.4.0",
		"eslint-plugin-prettier": "^3.1.1",
		"husky": "^3.0.9",
		"lint-staged": "^9.4.2",
		"prettier": "^1.18.2",
		"pretty-quick": "^2.0.0",
		"typescript": "^3.6.4",
		"yarn": "^1.19.1"
	},
	"engines": {
		"node": ">=11.0.0 <12.0.0",
		"yarn": "^1.9.0"
	},
	"peerDependencies": {
		"axios": "^0.19.0",
		"immutable": "^4.0.0-rc.12",
		"lodash": "^4.17.15",
		"react": "^16.9.9",
		"react-redux": "^7.1.1",
		"redux": "^4.0.4",
		"redux-saga": "^1.1.1",
		"reselect": "^4.0.0"
	}
}
